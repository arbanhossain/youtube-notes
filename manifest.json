{
    "name": "Youtube Notes",
    "version": "0.0.1",
    "description": "Take notes on youtube while watching",
    "background": {
        "service_worker": "src/background.js"
    },
    "permissions": [
        "tabs",
        "scripting",
        "storage",
        "activeTab",
        "declarativeContent"
    ],
    "action": {
        "default_popup": "src/popup/popup.html"
    },
    "manifest_version": 3
}